{% block page_scripts %}
    <!-- Wisembly App registration -->
    <script type="text/javascript">
        window.api_url = '{{ app['third_party']['wisembly'].api_url }}';
        window.app_id = '{{ app['third_party']['wisembly'].app_id }}';
        window.app_secret = '{{ app['third_party']['wisembly'].app_secret }}';
