<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js {% block html_class %}{% endblock %}" lang="{{ app.locale }}"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block page_title %}Wisembly{% endblock %}</title>
    <meta name="description" content="{% block meta_description %}Work together in meetings - Wisembly is a web & mobile solution that allows you to prepare, facilitate and monitor your meetings.{% endblock %}">
    <meta name="author" content="The Wisembly Team">
    <meta name="keywords" content="{% block meta_keywords %}wisembly,meeting,interactivity,confcalls, seminaries,training,facilitate,monitor,prepare{% endblock %}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <base href="{{ app.request.basePath }}/" />

    {% block page_css %}
        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,500,600,700' rel='stylesheet' type='text/css'>
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('static/sass/wisembly.css') }}">
    {% endblock %}
    <script src="/vendor/prefixfree/prefixfree.min.js" type="text/javascript"></script>
</head>
<body id='top' class='{% block body_class %}{% endblock %}' data-spy='scroll' data-target='.spy-list' data-offset='150'>

<a class="scroll" id="back-to-top" href="#top"><i class="icon-chevron-up"></i></a>

{% include 'includes/navigation.html.twig' %}

{% block content %}{% endblock %}

{% include 'includes/footer.html.twig' %}
</body>
</html>
