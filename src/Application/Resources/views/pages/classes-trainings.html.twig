{% extends 'layouts/main.html.twig' %}

{# load features localization keys #}
{% block features_training %}{% endblock %}

{% block body_class %}classes-trainings{% endblock %}

{# SEO #}
{% set page = 'classes_trainings' %}
{% block meta %}{% endblock %}
{% block page_title %}{{ app.meta[page].title }}{% endblock %}
{% block meta_description %}{{ app.meta[page].description }}{% endblock %}
{% block meta_keywords %}{{ app.meta[page].keywords }}{% endblock %}


{%  block content %}
	<div class='section header' style='background-image: url(http://cdn2.stocksy.com/a/t1K000/z0/76997.jpg);'>
		<div class='container'>
			<div class='row'>
				<div class='column'>
					<div class='column-inner'>
						<div class='id'>
							<h1><span>{{ 'classes_trainings_title' | trans }}</span></h1>
							<h2><span>{{ 'classes_trainings_desc1' | trans }}</span></h2>
							<h2><span>{{ 'classes_trainings_desc2' | trans }}</span></h2>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class='section features-list'>
		<div class='container'>
			<ul class='list'>
				<li class='one first'>
					<a href='#documents-management'>
						<i class='picto picto-documents-management'></i>
						<span class='medium'>{{ 'documents_management_link' | trans }}</span>
					</a>
				</li>
				<li class='two'>
					<a href='#quiz'>
						<i class='picto picto-students-evaluation'></i>
						<span class='medium'>{{ 'students_evaluation_link' | trans }}</span>
					</a>
				</li>
				<li class='three'>
					<a href='#'>
						<i class='picto picto-trainings-evaluation'></i>
						<span class='medium'>{{ 'trainings_evaluation_link' | trans }}</span>
					</a>
				</li>
				<li class='four'>
					<a href='#quizzes'>
						<i class='picto picto-quiz'></i>
						<span class='short'>{{ 'quiz_link' | trans }}</span>
					</a>
				</li>
				<li class='five'>
					<a href='#'>
						<i class='picto picto-qa'></i>
						<span class='short'>{{ 'qna_link' | trans }}</span>
					</a>
				</li>
				<li class='six'>
					<a href='#'>
						<i class='picto picto-stats-reporting'></i>
						<span class='medium'>{{ 'stats_reporting_link' | trans }}</span>
					</a>
				</li>
				<li class='seven'>
					<a href='#'>
						<i class='picto picto-multi-devices'></i>
						<span class='short'>{{ 'multi_devices_link' | trans }}</span>
					</a>
				</li>
			</ul>
		</div>
	</div>
	<div class='section documents-management' id='documents-management'>
		<div class='container'>
			<div class='row'>
				<div class='column six'>
					<div class='column-inner'>
						<h3>{{ 'documents_management_title' | trans }}</h3>
						<p>{{ 'documents_management_desc' | trans }}</p>
					</div>
				</div>
				<div class='column four last'>
					<div class='column-inner'>
						<div class='browser hide'>
							<div class='browser-bar'>
								<span>http://app.wisembly.com/demo</span>
							</div>
							<div class='browser-window'>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class='section try-now'>
		<div class='container'>
			<div class='row'>
				<div class='column three last'>
					<div class='column-inner'>
						<a href='{{ path('plans') }}' class='button button-large button-attention'>{{ 'try_for_free' | trans }}</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class='section students-evaluation' id='students-evaluation'>
		<div class='container'>
			<div class='row'>
				<div class='column five'>
					<div class='column-inner'>
						<h3><span>{{ 'students_evaluation_title' | trans }}</span></h3>
						<div class='row'>
							<div class='column six'>
								<div class='column-inner'>
									{{ 'students_evaluation_desc1' | trans | raw }}
								</div>
							</div>
							<div class='column six'>
								<div class='column-inner'>
									{{ 'students_evaluation_desc2' | trans | raw }}
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class='section try-now' id=''>
		<div class='container'>
			<div class='row'>
				<div class='column three last'>
					<div class='column-inner'>
						<a href='{{ path('plans') }}' class='button button-large button-attention'>{{ 'try_for_free' | trans }}</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class='section training-evaluation' id='training-evaluation'>
		<div class='container'>
