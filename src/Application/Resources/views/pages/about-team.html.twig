{% extends 'layouts/main.html.twig' %}

{% block body_class %}about-team{% endblock %}

{% block content %}
	<div class='section header' background-image: url();>
		<div class='container'>
			<h1>Notre équipe</h1>
			<h2>De loin la meilleure équipe à Paris</h2>
		</div>
	</div>
	<div class='section'>
		<div class='container'>
			<div class='row'>
				<div class='column three'>
					{% include 'includes/about-navigation.html.twig' %}
				</div>
				<div class='column eight last'>
					<div class='column-inner'>
						<ul class='list team-list'>
							<li>
								<div class='person'>
									<div class='img'>
										<img src='https://si0.twimg.com/profile_images/1226148948/gui_reasonably_small.png'>
									</div>
									<div class='name'>Guillaume Potier</div>
									<div class='role'>Founder & CTO</div>
								</div>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}